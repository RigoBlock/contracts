//! Recovery Interface contract.
//! By Gabriele Rigo (Rigo Investment Sagl), 2017.
//! Released under the Apache Licence 2.
//! Allow for recovery of funds/access in a backup multisig mode.

pragma solidity ^0.4.10;

contract RecoveryFace {

  event RecoveredOwner(address indexed _recoverer, address indexed _newOwner, address indexed _newRecoverer);
  
  function Recovery(address _recoverer) {} 
  function recoverOwner(address _newOwner) {}
  function setRecovery(address _newRecoverer) {}
  
  function getOwner(address) constant returns (address owner) {}
  function getRecovere(address) constant returns (address _recoverer) {}
}
